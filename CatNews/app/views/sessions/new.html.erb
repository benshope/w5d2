<h1>Login</h1>

<%= form_for(:session, url: signin_path) do |f| %>

      <%= f.label :name, "User Name" %>
      <%= f.text_field :name %>
      <br>
      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>
